type Review {
    reviewId: ID!
    userId: ID!
    nickname: String!
    title: String!
    content: String!
    images: [Image]!
    locationTags: String!
    themeTags: String!
    commentCount: Int!
    recommendCount: Int!
    reportCount: Int!
}

type Image {
    url: String!
    description: String!
}