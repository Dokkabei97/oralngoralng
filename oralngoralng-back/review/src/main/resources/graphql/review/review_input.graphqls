input CreateReviewRequest {
    userId: ID!
    nickname: String!
    title: String!
    content: String!
    images: [ImageInput]!
    locationTags: [LocationTag]!
    themeTags: [ThemeTag]!
}

input ImageInput {
    imageUrl: String!
    imageDescription: String!
}

input UpdateReviewRequest {
    reviewId: ID!
    userId: ID!
    title: String!
    content: String!
    images: [String]!
    locationTags: [LocationTag]!
    themeTags: [ThemeTag]!
}

input DeleteReviewRequest {
    reviewId: ID!
    userId: ID!
}